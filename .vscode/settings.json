{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.tabSize": 2,
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.vite": true,
    "**/dist": true,
    "**/out": true
  },
  "search.exclude": {
    "**/.vite": true,
    "**/dist": true,
    "**/out": true,
    "**/node_modules": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}